{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_MAPBOX_ACCESS_TOKEN": "$NEXT_PUBLIC_MAPBOX_ACCESS_TOKEN"
    }
  }
}
